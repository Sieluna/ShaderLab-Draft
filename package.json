{
    "name": "shaderlab",
    "version": "0.0.0",
    "private": true,
    "dependencies": {
        "express": "latest",
        "jsonwebtoken": "latest",
        "morgan": "latest",
        "multer": "latest",
        "mysql2": "latest",
        "sequelize": "latest"
    },
    "devDependencies": {
        "chai": "latest",
        "chai-http": "latest",
        "node-fetch": "latest",
        "form-data": "latest",
        "mocha": "latest",
        "webpack": "latest",
        "webpack-cli": "latest",
        "html-webpack-plugin": "latest"
    },
    "scripts": {
        "server": "set DEBUG=shaderlab:* && node ./sys/bin/www",
        "test": "mocha --recursive './sys/test/*.js' --reporter list",
        "build": "webpack --config ./sys/public/webpack.config.js",

        "compile-editor": "tsc --project ./sys/public/js/editor/tsconfig.json && set FILTER_PATH=sys/public/js/editor/dist && node ./sys/bin/suffix.js",
        "third-party": "node ./sys/public/js/bin/fetch.js"
    }
}
